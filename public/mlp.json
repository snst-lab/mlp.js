{
    "dom": "div,img,code,svg",
    "event": "click,mouseenter,mouseleave,touchstart,touchend",
    "screen": {
        "MOBILE": "screen.width / screen.height <= 11 / 16",
        "TABLET": "screen.width / screen.height > 11 / 16 && screen.width / screen.height < 13 / 16",
        "DESKTOP": "screen.width / screen.height >= 13 / 16"
    },
    "sanitize": true,
    "cookie": false,
    "debug": {
        "console": true,
        "fpsMonitor": false
    }
}