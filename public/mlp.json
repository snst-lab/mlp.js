{
    "dom":"div,span,input,img,svg,i,pre,code",
    "event":"click,focus,blur,change,mouseenter,mouseleave,touchstart,touchend,keyup,scroll",
    "screen":{
        "MOBILE": "screen.width / screen.height <= 11 / 16",
        "TABLET": "screen.width / screen.height > 11 / 16 && screen.width / screen.height < 13 / 16",
        "DESKTOP": "screen.width / screen.height >= 13 / 16 "
    },
    "sanitize": true,
    "cookie": false
}
